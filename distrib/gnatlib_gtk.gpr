with "gnatlib";
with "gtkada";
project GnatLib_Gtk is
   for Library_Name use "gnatlib_gtk";
   for Source_Dirs use ("../../include/gnatlib");
   for Externally_Built use "true";

   for Source_List_File use "gnatlib/" & Project'Library_Name & ".lgpr";
   for Library_Kind use GnatLib.GnatLib_Kind;
   for Library_Dir  use "../gnatlib/" & Project'Library_Kind;
   for Library_Version use
     "lib" & Project'Library_Name & ".so." & Gnatlib.Version;

end GnatLib_Gtk;

